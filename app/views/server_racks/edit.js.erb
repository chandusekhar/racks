$("#modal_form").html("<%= escape_javascript(render :partial => 'edit', :locals => {:server_rack => @server_rack}) %>");
$("#modal_form_for_server_rack<%= @server_rack.id %>").dialog({
	modal: true,
	close: function(event, ui) { 
		$("#edit_server_rack_<%= @server_rack.id %> > div.tabs ").tabs("destroy");
		$("#edit_server_rack_<%= @server_rack.id %> > div.tabs ").html(" ");
		$("#modal_form_for_server_rack<%= @server_rack.id %>").dialog("destroy");
		$("#modal_form").html("  ");
	 },
	width:400
	});
$('#edit_server_rack_<%= @server_rack.id %> > div.tabs ').tabs();
